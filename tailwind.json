{
	"relative": {
		"style": {
			"position": "relative"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"w-40": {
		"style": {
			"width": 160
		}
	},
	"flex-row": {
		"style": {
			"flexDirection": "row"
		}
	},
	"flex-col": {
		"style": {
			"flexDirection": "column"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-between": {
		"style": {
			"justifyContent": "space-between"
		}
	},
	"gap-2": {
		"style": {
			"gap": 8
		}
	},
	"rounded-md": {
		"style": {
			"borderTopLeftRadius": 6,
			"borderTopRightRadius": 6,
			"borderBottomRightRadius": 6,
			"borderBottomLeftRadius": 6
